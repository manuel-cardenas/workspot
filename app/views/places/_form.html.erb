<%= form_for(@place) do |f| %>
    <%= f.label :name, "Nombre" %>
    <%= f.text_field :name %>

    <%= f.label :address, "Dirección" %>
    <%= f.text_field :address %>

    <%= f.label :city, "Ciudad" %>
    <%= f.text_field :city %>

    <%= f.label :country, "País" %>
    <%= f.text_field :country %>

    <%= f.label :price, "Precio" %>
    <%= f.text_field :price %>

  <%= f.submit %>
<% end %>
